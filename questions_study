/home/kidd/workspace/bc/MissionImpossible/models/Mission.php
    /*
     * 挑战成功用户
     * */
    public function getSuccessors()
    {
        $challenges = $this->challenges;

        $users = array();
        foreach ($challenges as $challenge) {
            if ($challenge->isSucceed()) {
                $users[] = $challenge->user;
            }
        }

        return $users;
    }

