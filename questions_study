   public function getMission()
    {
        if ($this->mission_type == MissionType::PERIOD) {
            return $this->hasOne(PeriodMission::className(), ['id' => 'mission_id']);
        } else {
            return $this->hasOne(Mission::className(), ['id' => 'mission_id']);
        }
    }

